#include <stdio.h>


long long int TenToTwo(int a){
    int c=65536, i, s=0, f;
    for(i=0; i<=16; i++){
        f=c&a;
        if(f!=0)
            s=s*10+1;
        else
            s*=10;
        c/=2;
    }
    return s;
}


int IntInput(char c){
    int s=0;
    while(c>='0' && c<='9'){
        if((c>='0') && (c<='9'))
            s=s*10+c-'0';
        c=getchar();
    }
    return s;
}


int main(){
    char c;
    int a, i=0;
    printf("Enter a set of integers (a<=2**16) in one line, separating the elements with spaces\n");
    while((c=getchar())!='\n'){
        if((c>='0') && (c<='9')){
            a=IntInput(c);
            i++;
            printf("Element %d: %d - %d\n", i, a, TenToTwo(a));
        }
    }
    return 0;
}
